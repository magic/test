{
  "name": "@magic/test",
  "version": "0.2.0",
  "author": "Wizards & Witches",
  "description": "simple yet powerful unit testing library",
  "license": "AGPL-3.0",
  "homepage": "https://magic.github.io/test",
  "main": "src/index.mjs",
  "module": "src/index.mjs",
  "type": "module",
  "bin": {
    "t": "src/bin/t.mjs",
    "magic-test": "src/bin/t.mjs",
    "calls": "src/bin/calls.mjs"
  },
  "scripts": {
    "start": "node src/bin/t.mjs -p",
    "build": "NODE_ENV=production magic build",
    "prod": "NODE_ENV=production magic build serve",
    "clean": "magic clean",
    "serve": "magic serve",
    "dev": "magic dev",
    "test": "node src/bin/t.mjs",
    "calls": "node src/bin/calls.mjs",
    "format": "f -w --exclude docs",
    "format:check": "f --exclude docs"
  },
  "engines": {
    "node": ">=14.15.4"
  },
  "engineStrict": true,
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magic/test.git"
  },
  "bugs": {
    "url": "https://github.com/magic/test/issues"
  },
  "dependencies": {
    "@magic/cli": "0.0.37",
    "@magic/core": "0.0.133",
    "@magic/deep": "0.1.10",
    "@magic/error": "0.0.12",
    "@magic/fs": "0.0.21",
    "@magic/log": "0.1.12",
    "@magic/types": "0.1.18",
    "c8": "7.10.0",
    "coveralls": "3.1.1"
  },
  "devDependencies": {
    "@magic-modules/git-badges": "0.0.11",
    "@magic-modules/light-switch": "0.0.10",
    "@magic-modules/no-spy": "0.0.6",
    "@magic-modules/pre": "0.0.11",
    "@magic-themes/docs": "0.0.14",
    "@magic/format": "0.0.35"
  },
  "contributors": [
    {
      "name": "jascha ehrenreich",
      "github": "jaeh"
    }
  ]
}
