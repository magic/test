{
  "name": "@magic/test",
  "version": "0.1.30",
  "author": "Wizards & Witches",
  "description": "simple yet powerful unit testing library",
  "license": "AGPL-3.0",
  "homepage": "https://magic.github.io/test",
  "main": "src/index.mjs",
  "module": "src/index.mjs",
  "bin": {
    "t": "src/bin/t.mjs",
    "magic-test": "src/bin/t.mjs",
    "calls": "src/bin/calls.mjs"
  },
  "scripts": {
    "start": "node src/bin/t.mjs -p",
    "build": "NODE_ENV=production magic build",
    "prod": "NODE_ENV=production magic build serve",
    "clean": "magic clean",
    "serve": "magic serve",
    "dev": "magic dev",
    "test": "node src/bin/t.mjs",
    "calls": "node src/bin/calls.mjs",
    "format": "f -w --exclude docs",
    "format:check": "f --exclude docs"
  },
  "engines": {
    "node": ">=13.5.0"
  },
  "engineStrict": true,
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magic/test.git"
  },
  "bugs": {
    "url": "https://github.com/magic/test/issues"
  },
  "dependencies": {
    "@magic/cli": "0.0.15",
    "@magic/css": "0.7.9",
    "@magic/deep": "0.1.2",
    "@magic/fs": "0.0.2",
    "@magic/hyperapp": "2.0.3",
    "@magic/log": "0.1.3",
    "@magic/types": "0.1.7",
    "c8": "7.0.0",
    "coveralls": "3.0.9"
  },
  "devDependencies": {
    "@magic-modules/gdpr": "0.0.12",
    "@magic-modules/git-badges": "0.0.3",
    "@magic-modules/light-switch": "0.0.3",
    "@magic-modules/pre": "0.0.2",
    "@magic-themes/docs": "0.0.3",
    "@magic/core": "0.0.18",
    "@magic/format": "0.0.5"
  },
  "contributors": [
    {
      "name": "jascha ehrenreich",
      "github": "jaeh"
    }
  ]
}
