{
  "name": "test",
  "version": "0.0.1",
  "description": "declarative testing library",
  "main": "src/index.js",
  "scripts": {
    "start": "nyc -a node test/",
    "test": "VERBOSE=true nyc -a node test/",
    "test:multirun": "VERBOSE=true RUNS=10 node test/",
    "lint": "esw -w ./src ./test",
    "lint:fix": "eslint --fix ./src ./test"
  },
  "author": "Jascha Ehrenreich <jascha@jaeh.at>",
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "comma-dangle": ["error", "always-multiline"],
      "brace-style": ["error", "stroustrup"],
      "semi": ["error", "never"]
    }
  },
  "dependencies": {
    "deep": "git+http://git@localhost:7005/deep",
    "types": "git+http://git@localhost:7005/types"
  }
}
