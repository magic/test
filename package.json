{
  "name": "@magic/test",
  "version": "0.0.1",
  "description": "declarative testing library",
  "main": "src/index.js",
  "bin": {
    "t": "src/bin/t.js",
    "unit": "src/bin/unit.js",
    "coverage": "src/bin/coverage.js"
  },
  "scripts": {
    "start": "NODE_ENV=production ./src/bin/t.js",
    "test": "./src/bin/t.js",
    "test:multirun": "RUNS=10 ./src/bin/t.js",
    "format": "format --write",
    "format:check": "format --list-different"
  },
  "author": "Jascha Ehrenreich <jascha@jaeh.at>",
  "devDependencies": {
    "@magic/lint": "github:magic/lint"
  },
  "dependencies": {
    "@magic/log": "github:magic/log",
    "nyc": "11.6.0"
  }
}
